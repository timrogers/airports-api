{
  "name": "airports-api",
  "version": "1.0.0",
  "description": "A simple API for returning data about global airports, sourced from Duffel",
  "main": "server.js",
  "scripts": {
    "generate": "node scripts/generate_airports_json",
    "start": "node src/index.js",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "build": "tsc",
    "prettier-format": "prettier src/**/*.ts --write",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "author": "Tim Rogers <me@timrogers.co.uk>",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@duffel/api": "^1.8.2",
    "@tsconfig/node16": "^1.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.6.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "repository": "timrogers/airports-api"
}
