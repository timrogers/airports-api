{
  "name": "iata-code-decoder-api",
  "version": "1.0.0",
  "description": "A simple API, written in Node.js with the Express framework, which allows you to identify airports, airlines and aircraft by their IATA code",
  "scripts": {
    "generate-airports": "node scripts/generate_airports_json",
    "generate-airlines": "node scripts/generate_airlines_json",
    "generate-aircraft": "node scripts/generate_aircraft_json",
    "start": "node src/index.js",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "build": "tsc",
    "prettier": "prettier src/**/*.ts",
    "prettier-fix": "prettier src/**/*.ts --write",
    "eslint": "eslint . --ext .ts",
    "eslint-fix": "eslint . --ext .ts --fix"
  },
  "author": "Tim Rogers <me@timrogers.co.uk>",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@duffel/api": "^1.10.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.6.2",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.2"
  },
  "repository": "timrogers/iata-code-decoder-api"
}
